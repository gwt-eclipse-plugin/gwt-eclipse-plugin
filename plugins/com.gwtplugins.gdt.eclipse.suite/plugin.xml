      <!-- GWT Super Dev Mode Launch shortcut when using GWT -->
      <shortcut
            class="com.google.gdt.eclipse.suite.launch.ui.shortcuts.WebAppGWTSuperDevModeLaunchShortcut"
            description="Launch a Web Application using GWT super dev mode with DevMode"
            icon="icons/gwt_server_16x16.png"
            id="com.google.gdt.eclipse.suite.launch.ui.webappLaunchShortcutGwtSuperDevMode"
            label="GWT Web Application with DevMode (Super Dev Mode)"
            modes="run, debug">
         <contextualLaunch>
            <enablement>
               <with variable="selection">
                  <count value= "1"/>
                     <iterate>
                        <test property="com.google.gdt.eclipse.suite.isGwtLaunchTarget"/>
                     </iterate>
               </with>
            </enablement>
         </contextualLaunch>
         <description
               description="Run a Web Application with DevMode (SDM)"
               mode="run" />
         <description
               description="Debug a Web Application (GWT SDM)"
               mode="debug" />
         <configurationType
               id="com.google.gdt.eclipse.suite.webapp"/>
      </shortcut>
      <!-- GWT Classic Dev Mode Launch shortcut when using GWT -->
      <shortcut
            class="com.google.gdt.eclipse.suite.launch.ui.shortcuts.WebAppGWTClassicDevModeLaunchShortcut"
            description="Launch a Web Application using GWT classic DevMode"
            icon="icons/gwt_server_16x16.png"
            id="com.google.gdt.eclipse.suite.launch.ui.webappLaunchShortcutGwtClassicDevMode"
            label="GWT Web Application with DevMode (Classic Dev Mode)"
            modes="run, debug">
         <contextualLaunch>
            <enablement>
               <with variable="selection">
                  <count value= "1"/>
                     <iterate>
                        <test property="com.google.gdt.eclipse.suite.isGwtLaunchTarget"/>
                     </iterate>
               </with>
            </enablement>
         </contextualLaunch>
         <description
               description="Run a Web Application with DevMode (Classic Dev Mode)"
               mode="run" />
         <description
               description="Debug a Web Application With DevMode (Classic Dev Mode)"
               mode="debug" />
         <configurationType
               id="com.google.gdt.eclipse.suite.webapp"/>
      </shortcut>
   </extension>  
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="com.google.gdt.eclipse.suite.newWizards"
            name="%gpe.menu.label">
      </category>
      <wizard
            category="com.google.gdt.eclipse.suite.newWizards"
            class="com.google.gdt.eclipse.suite.wizards.NewWebAppProjectWizard"
            icon="icons/gdt-new-project_16x16.png"
            id="com.google.gdt.eclipse.suite.wizards.newProjectWizard"
            name="GWT Web Application Project"
            preferredPerspectives="org.eclipse.jdt.ui.JavaPerspective"
            project="true">
      </wizard>      
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="com.google.gdt.eclipse.suite.importWizards"
            name="%gpe.menu.label">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <newWizardShortcut
               id="com.google.gdt.eclipse.suite.wizards.newProjectWizard">
         </newWizardShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaBrowsingPerspective">
         <newWizardShortcut
               id="com.google.gdt.eclipse.suite.wizards.newProjectWizard">
         </newWizardShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaHierarchyPerspective">
         <newWizardShortcut
               id="com.google.gdt.eclipse.suite.wizards.newProjectWizard">
         </newWizardShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jst.j2ee.J2EEPerspective">
         <newWizardShortcut
               id="com.google.gdt.eclipse.suite.wizards.newProjectWizard">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <!--
   	Action set that other plugins will contribute actions to.
   -->
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            id="com.google.gdt.eclipse.suite.projectGoogleSettings"
            objectClass="org.eclipse.core.resources.IProject">
         <menu
               id="com.google.gdt.eclipse.suite.popupMenu.project"
               label="%gpe.menu.label"
               icon="icons/gdt_16x16.png"
               path="additions">
            <groupMarker
                  name="actionsContent">
            </groupMarker>
            <separator
                  name="separator1">
            </separator>
            <groupMarker
                  name="settingsContent">
            </groupMarker>
         </menu>
      </objectContribution>
      <objectContribution
            adaptable="true"
            id="com.google.gdt.eclipse.suite.fileActions"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               id="com.google.gdt.eclipse.suite.popupMenu.file"
               label="%gpe.menu.label"
               path="additions">
            <groupMarker
                  name="content">
            </groupMarker>
         </menu>
      </objectContribution>
   </extension>

   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
         id="com.google.gdt.eclipse.suite.launchTargetPropertyTester"
         namespace="com.google.gdt.eclipse.suite"
         properties="isLaunchTarget"
         type="org.eclipse.core.runtime.IAdaptable"
         class="com.google.gdt.eclipse.suite.propertytesters.LaunchTargetTester">
      </propertyTester>
      <propertyTester
         id="com.google.gdt.eclipse.suite.noServerLaunchTargetPropertyTester"
         namespace="com.google.gdt.eclipse.suite"
         properties="isNoServerLaunchTarget"
         type="org.eclipse.core.runtime.IAdaptable"
         class="com.google.gdt.eclipse.suite.propertytesters.NoServerLaunchTargetTester">
      </propertyTester>
      <propertyTester
            class="com.google.gdt.eclipse.suite.propertytesters.GwtLaunchTargetTester"
            id="com.google.gdt.eclipse.suite.propertytesters.gwtLaunchTargetTester"
            namespace="com.google.gdt.eclipse.suite"
            properties="isGwtLaunchTarget"
            type="org.eclipse.core.runtime.IAdaptable">
      </propertyTester>      
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?before=additions">
         <toolbar
               id="com.google.gdt.eclipse.suite.GDTPulldownToolbar"
               label="GWT">
            <command
                  commandId="com.google.gdt.eclipse.suite.GDTPulldown"
                  icon="icons/gdt_16x16.png"
                  label="Google Services and Development Tools"
                  style="pulldown">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:com.google.gdt.eclipse.suite.GDTPulldown?after=toolbarGroup.new">
         <command
               commandId="com.google.gdt.eclipse.suite.newWebAppProject"
               icon="icons/gdt-new-project_16x16.png"
               label="New GWT Application Project..."
               style="push"
               tooltip="New GWT Application Project">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:com.google.gdt.eclipse.suite.GDTPulldown">
         <separator
               name="toolbarGroup.new"
               visible="true">
         </separator>
         <separator
               name="toolbarGroup.import"
               visible="false">
         </separator>
         <separator
               name="toolbarGroup.compile"
               visible="true">
         </separator>
         <separator
               name="toolbarGroup.profile"
               visible="false">
         </separator>
         <separator
               name="toolbarGroup.deploy"
               visible="true">
         </separator>
         <separator
               name="toolbarGroup.list"
               visible="false">
         </separator>                                             
         <separator
               name="toolbarGroup.googleapis"
               visible="true">                                                   
         </separator>                  
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.google.gdt.eclipse.suite.actions.PulldownHandler"
            id="com.google.gdt.eclipse.suite.GDTPulldown"
            name="GDT Pulldown">
      </command>
      <command
            defaultHandler="com.google.gdt.eclipse.suite.actions.NewWebAppToolbarHandler"
            id="com.google.gdt.eclipse.suite.newWebAppProject"
            name="New GWT Application Project">
      </command>
   </extension>
</plugin>

